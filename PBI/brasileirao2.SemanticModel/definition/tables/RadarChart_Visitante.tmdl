	partition RadarChart_Visitante = calculated
		mode: import
		source = ```
				
				VAR TabelaBase =
				    SELECTCOLUMNS(
				        FILTER(
				            'full - unpivot',
				            'full - unpivot'[mandante_flag] = "N"
				        ),
				        "partida_id", 'full - unpivot'[id],
				        "clube", 'full - unpivot'[time],
				        "id_time", 'full - unpivot'[id_time]
				    )
				
				RETURN
				UNION(
				    -- Cálculo da Posse de Bola
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Posse de bola",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTime = [id_time]
				            RETURN
				                CALCULATE(
				                    MAX('campeonato-brasileiro-estatisticas-full'[posse_de_bola]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTime &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				    ),
				    
				    -- Cálculo da Precisão de Passes
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Precisão de passes",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTime = [id_time]
				            RETURN
				                CALCULATE(
				                    MAX('campeonato-brasileiro-estatisticas-full'[precisao_passes]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTime &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				    ),
				    
				    -- Cálculo de Passes % (ajustado para o visitante)
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Passes %",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTimeVisitante = [id_time]
				
				            VAR passesVisitante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[passes]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeVisitante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            VAR idTimeMandante =
				                CALCULATE(
				                    MIN('full - unpivot'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id] = partidaId &&
				                        'full - unpivot'[mandante_flag] = "S"
				                    )
				                )
				
				            VAR passesMandante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[passes]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeMandante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            VAR totalPartida = passesMandante + passesVisitante
				
				            RETURN DIVIDE(passesVisitante, totalPartida, 0)
				    ),
				    
				    -- Cálculo de Percentual de Finalizações (ajustado para o visitante)
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Percentual de finalizações",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTimeVisitante = [id_time]
				
				            -- Chutes no Alvo do Visitante
				            VAR chutesVisitante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[chutes_no_alvo]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeVisitante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Chutes no Alvo do Mandante
				            VAR idTimeMandante =
				                CALCULATE(
				                    MIN('full - unpivot'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id] = partidaId &&
				                        'full - unpivot'[mandante_flag] = "S"
				                    )
				                )
				
				            VAR chutesMandante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[chutes_no_alvo]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeMandante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Total de Chutes no Alvo
				            VAR totalChutes = chutesMandante + chutesVisitante
				
				            RETURN DIVIDE(chutesVisitante, totalChutes, 0)
				    ),
				    
				    -- Cálculo da Taxa de Eficácia (ajustado para "gols_pro" na tabela "full - unpivot" para o visitante)
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Taxa de eficácia",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTimeVisitante = [id_time]
				
				            -- Gols do Visitante
				            VAR golsVisitante =
				                CALCULATE(
				                    SUM('full - unpivot'[gols_pro]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeVisitante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Chutes no Alvo do Visitante
				            VAR chutesVisitante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[chutes_no_alvo]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeVisitante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Taxa de Eficácia
				            RETURN DIVIDE(golsVisitante, chutesVisitante, 0)
				    )
				)
				
				```

