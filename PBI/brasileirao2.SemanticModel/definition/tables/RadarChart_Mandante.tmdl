	partition RadarChart_Mandante = calculated
		mode: import
		source = ```
				
				VAR TabelaBase =
				    SELECTCOLUMNS(
				        FILTER(
				            'full - unpivot',
				            'full - unpivot'[mandante_flag] = "S"
				        ),
				        "partida_id", 'full - unpivot'[id],
				        "clube", 'full - unpivot'[time],
				        "id_time", 'full - unpivot'[id_time]
				    )
				
				RETURN
				UNION(
				    -- Cálculo da Posse de Bola
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Posse de bola",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTime = [id_time]
				            RETURN
				                CALCULATE(
				                    MAX('campeonato-brasileiro-estatisticas-full'[posse_de_bola]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTime &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				    ),
				    
				    -- Cálculo da Precisão de Passes
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Precisão de passes",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTime = [id_time]
				            RETURN
				                CALCULATE(
				                    MAX('campeonato-brasileiro-estatisticas-full'[precisao_passes]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTime &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				    ),
				    
				    -- Cálculo de Passes %
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Passes %",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTimeMandante = [id_time]
				
				            VAR passesMandante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[passes]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeMandante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            VAR idTimeVisitante =
				                CALCULATE(
				                    MIN('full - unpivot'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id] = partidaId &&
				                        'full - unpivot'[mandante_flag] = "N"
				                    )
				                )
				
				            VAR passesVisitante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[passes]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeVisitante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            VAR totalPartida = passesMandante + passesVisitante
				
				            RETURN DIVIDE(passesMandante, totalPartida, 0)
				    ),
				    
				    -- Cálculo de Percentual de Finalizações
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "% Finalizações",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTimeMandante = [id_time]
				
				            -- Chutes no Alvo do Mandante
				            VAR chutesMandante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[chutes_no_alvo]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeMandante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Chutes no Alvo do Visitante
				            VAR idTimeVisitante =
				                CALCULATE(
				                    MIN('full - unpivot'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id] = partidaId &&
				                        'full - unpivot'[mandante_flag] = "N"
				                    )
				                )
				
				            VAR chutesVisitante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[chutes_no_alvo]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeVisitante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Total de Chutes no Alvo
				            VAR totalChutes = chutesMandante + chutesVisitante
				
				            RETURN DIVIDE(chutesMandante, totalChutes, 0)
				    ),
				    
				    -- Cálculo da Taxa de Eficácia (ajustado para "gols_pro" na tabela "full - unpivot")
				    ADDCOLUMNS(
				        TabelaBase,
				        "atributo", "Taxa de eficácia",
				        "valor",
				            VAR partidaId = [partida_id]
				            VAR idTimeMandante = [id_time]
				
				            -- Gols do Mandante
				            VAR golsMandante =
				                CALCULATE(
				                    SUM('full - unpivot'[gols_pro]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeMandante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Chutes no Alvo do Mandante
				            VAR chutesMandante =
				                CALCULATE(
				                    SUM('campeonato-brasileiro-estatisticas-full'[chutes_no_alvo]),
				                    USERELATIONSHIP('full - unpivot'[id_time], 'campeonato-brasileiro-estatisticas-full'[id_time]),
				                    FILTER(
				                        'full - unpivot',
				                        'full - unpivot'[id_time] = idTimeMandante &&
				                        'full - unpivot'[id] = partidaId
				                    )
				                )
				
				            -- Taxa de Eficácia
				            RETURN DIVIDE(golsMandante, chutesMandante, 0)
				    )
				)
				
				```

