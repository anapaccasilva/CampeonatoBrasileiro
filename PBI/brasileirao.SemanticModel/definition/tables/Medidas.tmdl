	measure PartidasDefault = ```
			
			
			CONCATENATEX(
			    'fato_CBPartidas',  
			    VAR DataHora = FORMAT('fato_CBPartidas'[data], "DD/MM/YYYY") & " " & FORMAT('fato_CBPartidas'[hora], "HH:mm")
			    VAR Mandante = IF('fato_CBPartidas'[mandante_flag] = "S", MIN('fato_CBPartidas'[time]), MIN('fato_CBPartidas'[adversario]))
			    VAR MandantePlacar = IF('fato_CBPartidas'[mandante_flag] = "S", MIN('fato_CBPartidas'[gols_pro]), MIN('fato_CBPartidas'[gols_contra]))
			    VAR Visitante = IF('fato_CBPartidas'[mandante_flag] = "N", MIN('fato_CBPartidas'[time]), MIN('fato_CBPartidas'[adversario]))
			    VAR VisitantePlacar = IF('fato_CBPartidas'[mandante_flag] = "N", MIN('fato_CBPartidas'[gols_pro]), MIN('fato_CBPartidas'[gols_contra]))
			    VAR Arena = 'fato_CBPartidas'[arena]
			    VAR Estado = IF(ISBLANK('fato_CBPartidas'[mandante_Estado]), "", " (" & 'fato_CBPartidas'[mandante_Estado] & ")")
			    VAR PartidaID = 'fato_CBPartidas'[ID]
			
			    VAR Cores = IF(
			        NOT(ISBLANK(CALCULATE(
			            SELECTEDVALUE(dim_cores[C√≥digo Hexadecimal]), 
			            FILTER(
			                dim_cores, 
			                dim_cores[Cor] = SELECTEDVALUE(dim_clubes[Cores])
			            )
			        ))),
			        CALCULATE(
			            SELECTEDVALUE(dim_cores[C√≥digo Hexadecimal]), 
			            FILTER(
			                dim_cores, 
			                dim_cores[Cor] = SELECTEDVALUE(dim_clubes[Cores])
			            )
			        ),
			        "#2DCC70"  -- Cor padr√£o caso n√£o tenha cor v√°lida
			    )
			
			
			
			    RETURN
			    "<div style='background-color: " & "#2DCC70" & "; padding: 8px; margin-bottom: 8px; border: 2px solid " & "#2DCC70" & "; border-radius: 5px; box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3); font-size: 75%;'>" & 
			
			    "<div style='text-align: center; color: " & "#f1f1f1" & "; font-size: 14px;'>" & 
			    DataHora & 
			    "</div>" &
			
			    "<div style='text-align: center; font-size: 16px; font-weight: bold; color: " & "#f1f1f1" & ";'>" & 
			    Mandante & " " & 
			    MandantePlacar & " - " & 
			    VisitantePlacar & " " & 
			    Visitante & 
			    "</div>" &
			
			    "<div style='text-align: center; font-size: 12px; color: " & "#f1f1f1" & ";'> " & Arena  & Estado &  "</div>" &
			
			    "</div>",
			    UNICHAR(10)
			)
			
			```
		displayFolder: Clubes\Jogos
