	measure ScoreInfluenciaBalanceado = ```
			
			    VAR PorcentagemComGol = DIVIDE([JogosComGolPercentual], 100)
			    VAR PorcentagemComCartao = DIVIDE([JogosComCartaoPercentual], 100)
			    VAR ClutchGols = [GolsClutch]
			    VAR GolsNormais = [DesempenhoGolsFeitosNormal]
			
			    VAR ClutchScore = MIN(DIVIDE(ClutchGols, 5), 1)
			    VAR GolsScore = MIN(DIVIDE(GolsNormais, 10), 1)
			
			    VAR InfluenciaFinal =
			        (PorcentagemComGol * 0.4) +
			        (ClutchScore * 0.25) +
			        ((1 - PorcentagemComCartao) * 0.15) +
			        (GolsScore * 0.2)
			
			    VAR JogosAtleta = [JogosAtleta]
			
			    RETURN 
			        IF(
			            JogosAtleta > 0,
			            ROUND(InfluenciaFinal * 100, 1),
			            0
			        )
			
